
public class Ascii {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		System.out.println(
				  "┏━━Todo List━━━━┓\r\n"
				+ "┃ \r\n"
				+ "┃ Id : \"\r\n"
				+ "┃ Pw : \"\r\n"
				+ "┃ Ac : \"\r\n"
				+ "┃                 *\r\n"
				+ "┗━━━━━━━━━━━━━━━━┛\r\n");
		
		
		
		System.out.println("	╔═══━━━─── • ───━━━═══╗\r\n"
				+ "	\r\n"
				+ "	\r\n"
				+ "	╚═══━━━─── • ───━━━═══╝");
		System.out.println("111");
	System.out.println("|￣￣￣￣￣￣￣￣￣￣￣￣￣￣￣￣￣￣￣￣|\r\n"
			+ "  asdsadsadsadsasdasadsads ໒꒱*॰\r\n"
			+ "|＿＿＿＿＿＿＿＿＿＿＿＿＿＿＿＿＿＿＿＿|\r\n"
			+ "　　         ᕱ ᕱ  ||\r\n"
			+ "　           ( ᴖ ‧̫ ᴖ ||\r\n"
			+ "  　         /　つ  Φ\r\n"
			+ "");
		
		System.out.println("╭                                    ╮");
		System.out.println("╰                                    ╯");
	
	
	}
	
	

}
